<ol reversed class="postlist" style="counter-reset: start-from {{ (backloglistCounter or backloglist.length) + 1 }}">
{% for task in backloglist | reverse %}
  <li class="postlist-item{% if task.url == url %} postlist-item-active{% endif %}">
    <a href="{{ task.url | url }}" class="postlist-link">{% if task.data.title %}{{ task.data.title }}{% else %}<code>{{ task.url }}</code>{% endif %}</a>
    <time class="postlist-date" datetime="{{ task.date | htmlDateString }}">{{ task.date | readableDate }}</time>
    {% for tag in task.data.tags %}
      {%- if collections.tagList.indexOf(tag) != -1 -%}
      {% set tagUrl %}/tags/{{ tag }}/{% endset %}
      <a href="{{ tagUrl | url }}" class="post-tag">{{ tag }}</a>
      {%- endif -%}
    {% endfor %}
  </li>
{% endfor %}
</ol>
